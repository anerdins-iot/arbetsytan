DISCORD_TOKEN=
DISCORD_CLIENT_ID=
DATABASE_URL=postgresql://postgres:postgres@localhost:5432/myapp
REDIS_URL=redis://localhost:6379

# Web app URL - must be reachable from the bot container
# In Docker: http://web:3000 (if same network) or external URL
WEB_APP_URL=http://localhost:3000

# Must match INTERNAL_API_KEY in web/.env
INTERNAL_API_KEY=discord-bot-secret-key-12345

# S3/MinIO storage (for file uploads from Discord)
S3_ENDPOINT=http://localhost:9000
S3_ACCESS_KEY=minioadmin
S3_SECRET_KEY=minioadmin
S3_BUCKET=arbetsytan
S3_REGION=us-east-1
